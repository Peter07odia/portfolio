{
  "version": 2,
  "functions": {
    "index.py": {
      "runtime": "@vercel/python",
      "excludeFiles": "{static/images/**,mcp-vercel/**,attached_assets/**,__pycache__/**,*.md,test_*,*.cache/**,.git/**}"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ]
} 